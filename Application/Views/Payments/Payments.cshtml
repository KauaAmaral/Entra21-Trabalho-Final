@model List<Vehicle>

<table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Modelo</th>
                <th>Placa</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var vehicles in Model)
            {
                <tr>
                    <td>@vehicles.Model</td>
                    <td>@vehicles.LicensePlate</td>
                </tr>
            }
        </tbody>
    </table>

    <div class="page">
        <div class="page-content">
            <div class="row mb-3">
                <div class="text-end">
                    <button type="submit" class="btn btn-animate mb-2 btn-animate-side btn-primary">
                        <span>
                            <i class="icon wb-shopping-cart" aria-hidden="true"></i>Pagar
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>