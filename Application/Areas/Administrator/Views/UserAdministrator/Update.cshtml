<div class="modal fade" id="userUpdateModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Editar Usuario</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">x</button>
            </div>
            <div class="modal-body">

                <div class="form-group">
                    <label for="campo-email" class="form-label">Email</label>
                    <input type="text" id="campo-email" class="form-control" />
                </div>

                <div class="form-group">
                    <label for="campo-name" class="form-label">Nome Completo</label>
                    <input type="text" id="campo-name" class="form-control" />
                </div>

                <div class="form-group">
                    <label for="campo-cpf" class="form-label">CPF</label>
                    <input type="text" id="campo-cpf" class="form-control" />
